<h1><%= @user.name %>'s Page</h1> 
<p>Email: <%= @user.email %></p>

<h1>Created Events</h1>

<ul>
    <%= render partial: "/events/event", collection: @created_events, as: :event %>
</ul>

<h1>Upcoming Attended Events</h1>
<ul>
    <%= render partial: "/events/event", collection: @upcoming_events, as: :event %>
</ul>

<h1>Previous Attended Events</h1>
<ul>
    <%= render partial: "/events/event", collection: @previous_events, as: :event %>
</ul>